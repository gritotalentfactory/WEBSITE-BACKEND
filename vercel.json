{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "headers": [
    { "name": "Access-Control-Allow-Origin", "value": "*" },
    { "name": "Access-Control-Allow-Methods", "value": "GET, HEAD, POST, PUT, PATCH, DELETE" },
    { "name": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
  ]
}